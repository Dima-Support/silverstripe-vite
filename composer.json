  "type": "silverstripe-vendormodule",
  "require": {
    "php": "^8.1",
    "silverstripe/framework": "^5.1 || ^6.0",
    "silverstripe/cms": "^5.1 || ^6.0"
  },
  "autoload": {
    "psr-4": {
